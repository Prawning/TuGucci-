{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "*",
        "desitination": "/index.html"
      }
    ],
    "redirects": [ {
      "source": "*",
      "destination": "/",
      "type": 301
    } ]
  },
  "functions": {
    "source": "functions"
  }
}
